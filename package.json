{
  "name": "lough-5-site",
  "version": "1.0.0",
  "description": "A simple website for a local charity 5K race",
  "main": "index.html",
  "scripts": {
    "start": "serve dist",
    "build": "mkdir -p dist/assets/js && cp -r index.html location.html course.html results.html records.html assets dist/ && npx esbuild src/js/main.js --bundle --minify --outfile=dist/assets/js/bundle.js",
    "dev-js": "npx esbuild src/js/main.js --bundle --watch --outfile=assets/js/bundle.js",
    "dev": "browser-sync start --server --files '**/*'",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:headed": "cypress run --headed",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "generate-db": "node scripts/generate-runner-database.js",
    "generate-db:verbose": "node scripts/generate-runner-database.js --verbose",
    "generate-db:dry-run": "node scripts/generate-runner-database.js --dry-run",
    "assign-runner-ids": "node scripts/assign-runner-ids.js",
    "assign-runner-ids:dry-run": "node scripts/assign-runner-ids.js --dry-run",
    "review-duplicates": "node scripts/review-duplicates.js",
    "generate-masters-records": "node scripts/generate-masters-records.js",
    "generate-fastest-50": "node scripts/generate-fastest-50.js"
  },
  "keywords": [
    "charity",
    "race",
    "5k",
    "running",
    "alpine.js"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "cypress": "^15.5.0",
    "esbuild": "^0.19.5",
    "serve": "^14.2.1"
  },
  "dependencies": {
    "alpinejs": "^3.13.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
